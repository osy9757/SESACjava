package EX;

interface A_6 {
	public abstract void abc();
}
class C {
	void cde (A_6 a) {
		a.abc();
	}
}

public class EX04_AnonymousClass_4 {
	public static void main(String[] args) {
		C c = new C();
		
		A_6 a = new A_6() {
			public void abc() {
				System.out.println("input 매개변수 전달");;
			}
		};
		c.cde(a);
		
		c.cde(new A() {
			public void abc() {
				System.out.println("입력 매개변수 전달");
			}
		}
				);
	}
}
