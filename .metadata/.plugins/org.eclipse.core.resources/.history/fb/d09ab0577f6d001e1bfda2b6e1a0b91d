package oop1;

public class Point3D extends Point2D {
	private int z;

	public int getZ() {
		return z;
	}

	public void setZ(int z) {
		this.z = z;
	}	
	public void print() {		
		System.out.println("x = " + this.getX());
		System.out.println("y = " + this.getY());
		System.out.println("z = " + this.getZ());		
	}
}
