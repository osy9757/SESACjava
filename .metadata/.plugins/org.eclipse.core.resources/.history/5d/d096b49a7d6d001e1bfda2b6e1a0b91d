package OOP1;

import java.util.Scanner;

public class Point2D {
	private int x;
	private int y;
	
	public void setX(int x) {
		this.x = x;
	}
	public void setY(int y) {
		this.y = y;
	}
	
	public void printX1() {
		System.out.println(this.x);
	}
	public void printY1() {
		System.out.println(this.y);
	}
}
