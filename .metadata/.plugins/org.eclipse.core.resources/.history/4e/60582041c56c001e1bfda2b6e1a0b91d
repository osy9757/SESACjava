package control;

import java.util.Scanner;

public class WhileTest3 {
	public static void main(String[] args) {
		
		
		int s_point = 2;
		
		while(s_point <= 6) {
			int i = s_point;
			int j = 1;
			while(j<=9) {				
				while(i <= 9 && i < s_point + 4) {
					System.out.print( i + "*" + j + "=" + (i*j) + "\t");
					i += 1;
				}
				j += i;
			}
			s_point += 4;	
		}
	}
}
