package control;

import java.util.Scanner;

public class WhileTest3 {
	public static void main(String[] args) {
		
		
		int k = 2;
		
		while(k <= 6) {
			int i = k;
			int j = 1;
			while(j<=9) {				
				while(i <= 9 && i < k + 4) {
					System.out.print( i + "*" + j + "=" + (i*j) + "\t");
					i += 1;
				}
				j += i;
			}
			k += 4;	
		}
	}
}
