package control;

import java.util.Scanner;

public class ForTest3 {
	public static void main(String[] args) {
		for (int j = 1; j <= 9; j++) {
		    for (int k = 2; k <= 9; k += 4) { 
		        for (int i = k; i < k + 4 && i <= 9; i++) {
		            System.out.print(i + "*" + j + "=" + (i * j) + "\t");
		        }
		        System.out.println();
		    }
		    System.out.println();
		}		
		
	}
}
